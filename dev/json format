Service signin:
Request
		{
			"Email" : "email@example.com",
			"Password" : "***********"
		}

Response
		{
			"Login Successfull"
		}
		{
				"Failure",
				{
					"Invaild User Credentials"
				}
		}
		{
				"Failure",
				{
					 "User Unregistered"
				}
		}
		{
				"Failure",
				{
					 "User Account Locked"
				}
		}

---------------------------------------------------------------------

Service	Signup:
	Request
		{
			"Firstname" : "text",
			"Lastname" : "text",
			"Email" : "email@gmail.com",
			"Password" : "********",
			"Confirm Password" : "***********",
			"Phone" : "+91-99999999",
			"Gender" : "M/F",
			"DOB" : "DD/MM/YYYY",
			"State" : "text",
			"Country" : "text"
		}

	Response
			{
				"Successfull"
			}
			
			{
				"Failure",
				{
					"Email-ID already exits"
				}
			}

			{
				"Failure",
				{
					"Mobile number already exits"
				}
			}
			{
				"Failure",
				{
					"UserName exists"
				}
			}

------------------------------------------------------------------------
Ask Question:
	Request
	{
		"Enter your question" : "text",
		"Tag name" : "text"
	}

	Response
	{
		"success" :  "Question Uploaded Successfull"
	}

------------------------------------------------------------------------
Contact Us
	Request
	{
		"Name"   	: "text",
		"Email"  	: "email@example.com",
		"Message"	: "text",
	}

	Response
	{
		"success" :  "Message Sent"
	}


------------------------------------------------------------------------
Forget Password
	Request
	{
		"Enter Mailid" : "email@example.com",
	}

	Response
	{
		"success" : "OTP Sent to your Email"
	}
	{
		"Failure" : "Check your Email Entered"
	}
			
	Request
	{
		"Enter OTP"	   : "999999",
	}


Reset Password
	Request
	{
		"Enter New Password" : "*******" ,
		"Confirm Password"	 : "*******" ,
	}

	Response
	{
		"success" : "Password Reset Successfull"
	}

	{
		"Failure" : "Invaild Password"
	}
-----------------------------------------------------------------------------
Post Answer
	Request
	{
		"Answer"		: "text" ,
	}

    Response	
	{
		"Answer Uploaded Successfull"
	}

	{
		"Failure" : "Answer Uploaded Failed"
	}
-----------------------------------------------------------------------------
Employee Registeration
Request
	{
		"First Name"	: "text" ,
		"Last Name"		: "text" ,
		"Gender"		: "M/F" ,
		"Joining Year"  : "yyyy" ,
		"Qualification": "text" ,
		"Email Id"		: "email@example.com" ,
		"password"		: "********" ,
		"phone" 		: "+91-999999999" ,
	}


	Response
	{
		"success" :  "Registeration Successfull"
	}

	Same as User registeration.
	{
		"Failure" : "Check the fields Entered"
	}
